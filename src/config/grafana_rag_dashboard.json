{
  "id": null,
  "title": "EKA RAG Monitoring Dashboard",
  "tags": ["fastapi", "prometheus", "rag", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Total API Requests",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "sum(eka_request_count_total)",
          "legendFormat": "All Endpoints",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["sum"], "fields": "", "values": false},
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value"
      }
    },
    {
      "type": "graph",
      "title": "Request Count (per Endpoint)",
      "gridPos": {"x": 6, "y": 0, "w": 18, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(eka_request_count_total[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "Request Latency (95th percentile)",
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(eka_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "RAG Retrieval Time",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "retrieval time (p95)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "RAG Inference Time",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_inference_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "inference time (p95)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2
    },
    {
      "type": "timeseries",
      "title": "Overall System Latency Overview",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(eka_request_latency_seconds_sum[1m])) / sum(rate(eka_request_latency_seconds_count[1m]))",
          "legendFormat": "avg latency (s)",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {"list": []},
  "time": {"from": "now-1h", "to": "now"}
}
